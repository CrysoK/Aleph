// Prueba la capacidad de recursividad en funciones

fn Fib(n) {
  if n > 1 {
    return Fib(n - 2) + Fib(n - 1)
  } else {
    return n
  }
}

let i = 0
while i < 20 {
  println(Fib(i))
  i += 1
}

