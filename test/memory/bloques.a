// Prueba de liberaci√≥n de memoria en bloques

fn Fib(n) {
  if n <= 1 {
    return n
  } else {
    return Fib(n - 2) + Fib(n - 1)
  }
}

fn first_n_fib(n) {
  let i = 0
  let res = []
  while i < n {
    res += [Fib(i)]
    i += 1
  }
  return res
}

for e in first_n_fib(15) {
  print(e, " ")
}
